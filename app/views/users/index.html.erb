
<div style="height:50px;width:100%;background-color:#f7f7f7;padding:20px">
  <h3>Obviously</h3>
</div>

<div style="width:100%;padding:20px">


  <p id="notice"><%= notice %></p>

  <h1>Listing Users</h1>

  <%= render partial: "user_links" %>

  <br/>
  <br/>

  <table cellpadding="7" cellspacing="0" border="0">
    <thead>
      <tr >
        <th>Username</th>
        <th>First name</th>
        <th>Last name</th>
      </tr>
    </thead>

    <tbody>
      <% @users.each do |user| %>
        <tr>
          <td><%= user.username %></td>
          <td><%= user.first_name %></td>
          <td><%= user.last_name %></td>
        </tr>
      <% end %>
    </tbody>
  </table>

  <br/>
  <br/>

  <%= render partial: "user_links" %>

</div>

<div style="height:50px;width:100%;background-color:#f7f7f7;padding:20px;margin-top:20px;bottom:0;position:absolute;">
  <h3>Obviously</h3>
</div>